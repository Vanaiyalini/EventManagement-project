<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>EventManagement</display-name>
  <welcome-file-list>
    <welcome-file>demo.jsp</welcome-file>
  </welcome-file-list>
  <display-name>ManageEvent</display-name>
  <welcome-file-list>
    <welcome-file>AvailableEvent.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <servlet-name>BookEventHandler</servlet-name>
    <servlet-class>bookevent.BookEventHandler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookEventHandler</servlet-name>
    <url-pattern>/BookEventHandler</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>MyFilter</display-name>
    <filter-name>MyFilter</filter-name>
    <filter-class>bookevent.MyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>MyFilter</filter-name>
    <url-pattern>/MyFilter</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>MyServlet</display-name>
    <servlet-name>MyServlet</servlet-name>
    <servlet-class>bookevent.MyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyServlet</servlet-name>
    <url-pattern>/MyServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EventFilter</display-name>
    <servlet-name>EventFilter</servlet-name>
    <servlet-class>addEventPackage.EventFilter</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventFilter</servlet-name>
    <url-pattern>/EventFilter</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>EventServlet</display-name>
    <servlet-name>EventServlet</servlet-name>
    <servlet-class>addEventPackage.EventServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EventServlet</servlet-name>
    <url-pattern>/EventServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description>Handles user feedback</description>
    <servlet-name>FeedbackProjectServlet</servlet-name>
    <servlet-class>com.yourpackage.FeedbackProjectServlet</servlet-class>
    <multipart-config>
      <max-file-size>5242880</max-file-size>
      <max-request-size>10485760</max-request-size>
      <file-size-threshold>1048576</file-size-threshold>
    </multipart-config>
  </servlet>
  <servlet-mapping>
    <servlet-name>FeedbackProjectServlet</servlet-name>
    <url-pattern>/FeedbackProject</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>login</display-name>
    <servlet-name>login</servlet-name>
    <servlet-class>AdmiAdd.login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
</web-app>